    <style>
        body {
            font-family: 'Inter', sans-serif;
            /* Added dummy content to show the popup over a page */
            background-color: #f1f5f9; /* slate-100 */
            height: 200vh;
        }
        
        /* Custom scrollbar for a cleaner look */
        .chat-messages::-webkit-scrollbar {
            width: 6px;
        }
        .chat-messages::-webkit-scrollbar-track {
            background: #f1f5f9; /* slate-100 */
        }
        .chat-messages::-webkit-scrollbar-thumb {
            background: #cbd5e1; /* slate-300 */
            border-radius: 3px;
        }
        .chat-messages::-webkit-scrollbar-thumb:hover {
            background: #94a3b8; /* slate-400 */
        }

        /* Animations for popup and messages */
        @keyframes slideInUp {
            from { 
                opacity: 0;
                transform: translateY(20px);
            }
            to { 
                opacity: 1;
                transform: translateY(0);
            }
        }
        @keyframes fadeIn {
            from { opacity: 0; }
            to { opacity: 1; }
        }
        .message {
            animation: slideInUp 0.3s ease-out;
        }
        .chat-popup {
            animation: slideInUp 0.4s ease-out;
        }
        .fab-button {
             animation: fadeIn 0.4s ease-out;
        }

        /* Typing indicator animation */
        .typing-dot {
            animation: typing-bubble 1.5s infinite;
        }
        @keyframes typing-bubble {
            0%, 60%, 100% { transform: translateY(0); }
            30% { transform: translateY(-4px); }
        }


        /* A custom gradient for a more unique look */
        .hero-gradient {
            background-color: #0f172a; /* fallback slate-900 */
            background-image: radial-gradient(at 27% 37%, hsla(215, 98%, 61%, 0.1) 0px, transparent 50%), 
                              radial-gradient(at 97% 21%, hsla(125, 98%, 72%, 0.1) 0px, transparent 50%), 
                              radial-gradient(at 52% 99%, hsla(355, 98%, 71%, 0.1) 0px, transparent 50%), 
                              radial-gradient(at 10% 29%, hsla(256, 96%, 68%, 0.1) 0px, transparent 50%), 
                              radial-gradient(at 97% 96%, hsla(38, 60%, 74%, 0.1) 0px, transparent 50%), 
                              radial-gradient(at 33% 50%, hsla(222, 67%, 73%, 0.1) 0px, transparent 50%), 
                              radial-gradient(at 79% 53%, hsla(343, 68%, 79%, 0.1) 0px, transparent 50%);
        }

        /* Subtle glow effect for the image container */
        .image-glow {
            box-shadow: 0 0 40px rgba(59, 130, 246, 0.3), 0 0 80px rgba(59, 130, 246, 0.2);
        }
    </style>
</head>

